package prasad;
import java.sql.*;

public class select_cmd {

	public static void main(String[] args) {
		Connection con = null;
		try {
			Class.forName("com.mysql.cj.jdbc.Driver");
			con = DriverManager.getConnection("jdbc:mysql://localhost:3306/orginal" , "root" , "raj123");
			Statement stat = con.createStatement();
			String sql = "SELECT * FROM title";
			ResultSet rs = stat.executeQuery(sql);
			System.out.println("Connection Created");
			
			while(rs.next()) {
				int w_id = rs.getInt("WORKER_REF_ID");
				String title = rs.getString("WORKER_TITLE");
				
				System.out.println("Worker id: " + w_id);
				System.out.println("Worker title: " + title);
				
			}
			rs.close();
			stat.close();
			con.close();
		}
		catch(Exception e) {
			System.err.println("Exception :" + e.getMessage());
		}
	}

}
